    <section class="ftco-section">
			<div class="container">
				<div class="row">
					<div class="col-lg-3">
						<div class="search-wrap">
							<h3>Advance Search</h3>
							<form action="/searchad" method="POST" class="search-property">
		        		<div class="row">
		        			<div class="col-md-12 align-items-end ftco-animate">
		        				<div class="form-group">
		        					<label for="location">Location</label>
		          				<div class="form-field">
		          					<div class="icon"><span class="icon-pencil"></span></div>
				                <input type="text" name="location" class="form-control" value="Kothrud">
				              </div>
			              </div>
		        			</div>
		        			<div class="col-md-12 align-items-end ftco-animate">
		        				<div class="form-group">
		        					<label for="accomodation">Accomodation Type</label>
		        					<div class="form-field">
		          					<div class="select-wrap">
		                      <div class="icon"><span class="ion-ios-arrow-down"></span></div>
		                      <select name="accomodation" id="" class="form-control">
		                        <option value="PG">PG</option>
		                        <option value="Hostel">Hostel</option>
		                        <option value="Flat">Flat</option>
		                        <option value="Bungalow">Bungalow</option>
		                      </select>
		                    </div>
				              </div>
			              </div>
		        			</div>
		        		</div>

		        		<div class="row">
		        			<div class="col-md-12 align-items-end ftco-animate">
		        				<div class="form-group">
		        					<label for="accomodation_type">Accomodation Availability</label>
		        					<div class="form-field">
		          					<div class="select-wrap">
		                      <div class="icon"><span class="ion-ios-arrow-down"></span></div>
		                      <select name="accomodation_type" id="" class="form-control">
		                      	<option value="1 Bed">1 Bed</option>
		                        <option value="2 Bed">2 Bed</option>
		                        <option value="3 Bed">3 Bed</option>
		                        <option value="4 Bed">4 Bed</option>
                                <option value="1 BHK">1 BHK</option>
                                <option value="2 BHK">2 BHK</option>
                                <option value="3 BHK">3 BHK</option>
		                      </select>
		                    </div>
				              </div>
			              </div>
		        			</div>
		        			<div class="col-md-12 align-items-end ftco-animate">
		        				<div class="form-group">
		        					<label for="gender">Gender</label>
		        					<div class="form-field">
		          					<div class="select-wrap">
		                      <div class="icon"><span class="ion-ios-arrow-down"></span></div>
		                      <select name="gender" id="" class="form-control">
		                      	<option value="Male">Male</option>
		                        <option value="Female">Female</option>
		                        <option value="Co-Ed">Co-Ed</option>
		                      </select>
		                    </div>
				              </div>
			              </div>
		        			</div>
		        			<div class="col-md-12 align-items-end ftco-animate">
		        				<div class="form-group">
		        					<label for="in_time">In-Time</label>
		        					<div class="form-field">
		          					<div class="select-wrap">
		                      <div class="icon"><span class="ion-ios-arrow-down"></span></div>
		                      <select name="in_time" id="" class="form-control">
		                      	<option value="2">5:00-18:00</option>
                                <option value="3">5:00-19:00</option>
                                <option value="4">6:00-19:00</option>
		                        <option value="5">6:00-20:00</option>
								<option value="6">6:00-21:00</option>
                                <option value="7">6:00-22:00</option>
                                <option value="8">6:00-23:00</option>
								<option value="9">Flexible</option>
		                      </select>
		                    </div>
				              </div>
			              </div>
		        			</div>
		        			<div class="col-md-12 align-self-end ftco-animate">
		        				<div class="form-group">
		        					<div class="form-field">
				                <input type="submit" value="Search" class="form-control btn btn-primary">
				              </div>
			              </div>
		        			</div>
		        		</div>
		        	</form>
		        </div>
					</div><!-- end -->
			<div class="col-lg-9">
		        <div class="row">
                    <% if(properties.length > 0){ %>
                        <h3>Properties available in <%=loc%></h3>
                    <% } else{ %>  
                        <h3>No Properties matched for your search!!</h3>
                    <% } %>
                    
                </div> 
                <div class="row">
					<% properties.forEach(function(i){ %>
						<div class="col-md-6 col-lg-4 ftco-animate">
		    				<div class="project">
		    					<div class="img">
				    				<img src=<%= i.Image_path %> class="img-fluid" alt="Colorlib Template">
				    				<div class="text">
				    					<!-- <span>Sale</span> -->
				    					<h3><a href="/<%= i.Residence_ID %>"><%=i.Residence_Name %></a></h3>
				    				</div>
			    				</div>
			    				<div class="desc pt-3">
			    					<!-- <h4 class="price">$120,000</h4> -->
										<p class="h-info"><span class="location"></span><%=i.Area_Name%></p>
			    				</div>
		    				</div>
		    			</div>
					<% }) %>
		    			
		    			
		    		</div>
		    		
		    	</div>
	    	</div>
			</div>
		</section>
		

    <footer class="ftco-footer ftco-section">
      <div class="container">
        <div class="row mb-5">
          <div class="col-md">
            <div class="ftco-footer-widget mb-4">
              <h2 class="ftco-heading-2">Accomodate.</h2>
              <p style="margin-top: -17px">Your Final Destination.</p>
              <ul class="ftco-footer-social list-unstyled float-md-left float-lft mt-3">
                <li class="ftco-animate"><a href="#"><span class="icon-twitter"></span></a></li>
                <li class="ftco-animate"><a href="#"><span class="icon-facebook"></span></a></li>
                <li class="ftco-animate"><a href="#"><span class="icon-instagram"></span></a></li>
              </ul>
            </div>
          </div>
        </div>
        
      </div>
    </footer>
    
  

  <!-- loader -->
  <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee"/><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00"/></svg></div>


